				<dependency id="Blazored.LocalStorage" Version="4.4.0"  />
				<dependency id="Mendi.Blazor.TrueSPA.Common" Version="0.0.1"  />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="content\js\NavigatorAccessor.js" target="_content\Mendi.Blazor.TrueSPA.NavEngine\js\NavigatorAccessor.js" />
